<%- include('./components/partials/header') -%>
<%- include('./components/partials/navbar') -%>

<!-- Main Content -->
        <div class="container pt-10">
            <div class="row gap-y">
              <%MenuItems.map(function(items){%>
              <div class="col-lg-3 col-sm-4">
                <div class="product-3">
                    <a href="/item/<%= items._id %>" class="product-media modal-trigger card shadow-1 hover-shadow-6">
                      <img src="<%= items.image%>" alt="product">
                    </a>
                    <div class="product-detail">
                      <h6><%= items.name%></h6>
                      <div class="product-price"><%= items.price%> FCFA</div>
                    </div>
                </div>
              </div>
                <%})%>
          </div>
    </div>
<%- include('./components/partials/cardModal') -%>
<%- include('./components/partials/footer') -%>






